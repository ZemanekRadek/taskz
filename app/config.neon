php:
	date.timezone: Europe/Prague

application:
	mapping:
		*: App\*Module\Presenters\*Presenter

database:
	default:
		dsn:          "mysql:host=127.0.0.1;dbname=taskz"
		user:         "root"
		password:     ""
		options:      [PDO::MYSQL_ATTR_COMPRESS = true]
		debugger:     true        # panel v debugger baru
		explain:      true        # explain dotaz≈Ø v debugger bar
		autowired:    true
		
services:
	- App\Model\Authenticator
	- App\Model\User
